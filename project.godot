config_version=5

[application]

config/name="Mission Spud"
run/main_scene="res://Menu/menu.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

SceneTransition="*res://UI/scene_transition.tscn"
Global="*res://World/global.gd"
PauseCanvas="*res://Menu/pause_canvas.tscn"
TimeSystem="*res://UI/time_system.tscn"
InteractionManager="*res://Interaction/interactionManager.tscn"
RobotShop="*res://UI/robot_shop.tscn"
RobotResearchStation="*res://UI/robot_research_station.tscn"
ResourceResearchStation="*res://UI/resource_research_station.tscn"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
SelectionManager="*res://Selection/selection_manager.tscn"
SidePanel="*res://UI/side_panel.tscn"
GatherNumbers="*res://UI/gatherNumbers.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/beehave/plugin.cfg")

[gui]

theme/custom_font="res://font/Bungee-Regular.ttf"
theme/default_font_multichannel_signed_distance_field=true

